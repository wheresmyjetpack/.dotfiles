# Create a new directory and enter it
function mk() {
          mkdir -p "$@" && cd "$@"
}

function zombiehunt() {
        ps alx | awk '{ print $4, $10 }' | grep -i 's'
}

function lipsum() {
        curl -silent http://lipsum.com | grep '<p>"Lorem' | sed -r -e 's/"//g' -e 's|<[a-zA-Z0-9\/]+>||g'
}
